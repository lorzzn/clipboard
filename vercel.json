{
  "version": 2,
  "builds": [
    {
      "src": "/src/pkgs/serverless-functions/*",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/f(/.*)?",
      "dest": "/src/pkgs/serverless-functions"
    }
  ]
}
